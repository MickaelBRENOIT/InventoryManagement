<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<GridPane fx:controller="com.mickaelbrenoit.controller.ControllerLogIn"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Label text="Username" GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.halignment="RIGHT"></Label>
    <TextField GridPane.rowIndex="0" GridPane.columnIndex="1"></TextField>
    <Label text="Password" GridPane.rowIndex="1" GridPane.columnIndex="0" GridPane.halignment="RIGHT"></Label>
    <PasswordField GridPane.rowIndex="1" GridPane.columnIndex="1"></PasswordField>
    <Button text="Log In" GridPane.rowIndex="2" GridPane.columnIndex="1" GridPane.halignment="RIGHT"></Button>
</GridPane>